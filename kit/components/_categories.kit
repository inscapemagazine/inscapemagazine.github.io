<div class="categories">
  <div class="categories__wrapper">
    <div class="theme">
      <amp-list width="auto"
        height="18"
        layout="fixed-height"
        src="https://script.google.com/a/sjcatholicschool.org/macros/s/AKfycbxA6l7DAG6U6K1RyLO6JaVqi-JIrDnG-c6Q8GE84xuOJJasLaXz/exec?command=Create-JSON&sheet=Year"
        class="theme__heading-container">
        <template type="amp-mustache" id="template-year">
          <h2 class="theme__heading heading">{{year}} Theme</h2>
        </template>
      </amp-list>
      <amp-list width="auto"
        height="107"
        layout="fixed-height"
        src="https://script.google.com/a/sjcatholicschool.org/macros/s/AKfycbxA6l7DAG6U6K1RyLO6JaVqi-JIrDnG-c6Q8GE84xuOJJasLaXz/exec?command=Create-JSON&sheet=Theme"
        class="">
        <template type="amp-mustache" id="template-year">
          <p class="theme__paragraph">{{theme}}</p>
        </template>
        <div overflow
          role="button"
          aria-label="Show more"
          class="theme__overflow-container">
          <p class="theme__overflow">
            <span>
              <amp-img class="theme__overflow-arrow"
                src="/icons/other/arrows-24px-glyph-1_bold-right.svg"
                width="8"
                height="8"
                layout="fixed"
                alt="Read More Arrow">
              </amp-img>
            </span>
            <span class="underline">Read more</span>
          <p>
        </div>
      </amp-list>
    <!-- @import "/kit/components/types" -->
    </div>
  </div>
</div>

